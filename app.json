{
  "expo": {
    "name": "onemor",
    "slug": "onemor",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/onemor-logo.png"
    },
    "splash": {
      "image": "./assets/images/onemor-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#4631de"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/onemor-logo.png",
        "backgroundColor": "#4631de"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/onemor-logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
